		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<Description>LibGit2Sharp implementations for managing local git repositories.</Description>
		<PackageTags>XTI;XTI_Git;Git</PackageTags>
	</PropertyGroup>

	<Import Project="..\\..\\Common.Targets"/>

	<ItemGroup>
		<PackageReference Include="LibGit2Sharp" Version="0.26.2" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\XTI_Git.Abstractions\XTI_Git.Abstractions.csproj" />
		<ProjectReference Include="..\XTI_Git\XTI_Git.csproj" />
	</ItemGroup>

</Project>
